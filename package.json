{
  "name": "creditech",
  "version": "1.0.0",
  "description": "CREDITECH - Comparação inteligente de taxas de crédito usando IA e dados do Banco Central",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "node-cron": "^3.0.3",
    "moment": "^2.29.4",
    "@tensorflow/tfjs-node": "^4.15.0",
    "ml-matrix": "^6.10.7",
    "ml-kmeans": "^6.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "crédito",
    "financiamento",
    "empréstimo",
    "banco central",
    "IA",
    "comparação",
    "fintech"
  ],
  "author": "CREDITECH Team",
  "license": "MIT"
}
